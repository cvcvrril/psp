<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.pantallas.list.ListController">
    <center>
    </center>
    <top>
        <MenuBar fx:id="menuPrincipal">
            <menus>
                <Menu fx:id="menuOptions" mnemonicParsing="false" text="Options">
                    <items>
                        <MenuItem fx:id="menuItemPantalla1" mnemonicParsing="false" onAction="#menuClick" text="Logout" />
                        <MenuItem fx:id="menuItemExit" mnemonicParsing="false" onAction="#exit" text="Exit" />
                    </items>
                </Menu>
                <Menu fx:id="menuCustomers" mnemonicParsing="false" text="Customers">
                    <items>
                        <MenuItem fx:id="menuItemUndo" mnemonicParsing="false" onAction="#cambiarcss" text="List" />
                        <MenuItem fx:id="menuItemRedo" mnemonicParsing="false" text="Add" />
                        <MenuItem fx:id="menuItemCut" mnemonicParsing="false" text="Update" />
                        <MenuItem fx:id="menuItemCopy" mnemonicParsing="false" text="Delete" />
                    </items>
                </Menu>
                <Menu fx:id="menuOrders" mnemonicParsing="false" text="Orders">
                    <items>
                        <MenuItem fx:id="menuItemUndo" mnemonicParsing="false" onAction="#cambiarcss" text="List" />
                        <MenuItem fx:id="menuItemRedo" mnemonicParsing="false" text="Add" />
                        <MenuItem fx:id="menuItemCut" mnemonicParsing="false" text="Update" />
                        <MenuItem fx:id="menuItemCopy" mnemonicParsing="false" text="Delete" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </top>
</BorderPane>
